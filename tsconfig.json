{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@model/*": [
        "./model/*"
      ],
      "@ctr/*": [
        "./ctr/*"
      ],
      "@constant/*": [
        "./constant/*"
      ]
    }
  }
}