{
  "compilerOptions": {
    "module": "commonjs",
    "baseUrl": "./",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "target": "es2016",
    "paths": {
      "@ctr/*": [
        "src/ctr/*"
      ],
      "@model/*": [
        "src/model/*"
      ],
      "@constant/*": [
        "src/constant/*"
      ]
    }
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules"]
}